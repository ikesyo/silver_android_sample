<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		package="com.ikesyo.silver_android_sample">
  <!-- the android:debuggable="true" attribute is overwritten by the compiler when the debug info option is set -->
  <application 
		android:label="@string/app_name"
		android:icon="@drawable/icon"
		android:debuggable="true">
	<activity android:label="@string/app_name" android:name="com.ikesyo.silver_android_sample.MainActivity">
	  <intent-filter>
		<action android:name="android.intent.action.MAIN" />
		<category android:name="android.intent.category.LAUNCHER" />
	  </intent-filter>
	</activity>
  </application>
  <uses-sdk android:minSdkVersion="19" />
</manifest>
